(*testmd-10 insertion sort on array*)

(*testmd-11 some operations of array of classes *)

(* The following sorts a list of ArrayClasses based on their val *)

program test;

class ArrayClass
BEGIN
    VAR
        val : integer;
FUNCTION happy(aa, ii, cc : integer ; bb : boolean) : integer;
VAR
    aa : ARRAY [0..19] of ArrayClass;
    ii, jj, tmp : integer;
    tmpC : ArrayClass;
BEGIN
    right := aa.bb(1) + cc;

    here := aa.bb(aa.bb(1), bb + 1, cc - 22, cc*cc)
END;

FUNCTION classmethod;
BEGIN
        diff := aa.bb(2+1, cc-dd) + cc;

    stuff := aa.bb(test, bb + 1, cc - 22, cc*cc)
END

END

class test
BEGIN   
VAR
    aa : ARRAY [0..19] of ArrayClass;
    ii, jj, tmp : integer;
    tmpC : ArrayClass;
FUNCTION test;
BEGIN
    diff := aa.bb(2+1, cc-dd) + cc;

stuff := aa.bb(test, bb + 1, cc - 22, cc*cc)
    
END;

FUNCTION staticmethod;
BEGIN
        diff := aa.bb(2+1, cc-dd) + cc;

    stuff := aa.bb(test, bb + 1, cc - 22, cc*cc)
END

END
.
