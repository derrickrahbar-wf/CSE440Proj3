program testIfThenElseNested;

class testIfThenElseNested

BEGIN
   VAR a1, b1, d1, e1, f1, y1, z1 : integer;
    aa, bb, cc, dd, ee, ff, xx, yy, zz, c1, c2, c3, c4, c5, c6: integer;
    gg, kk, ll, mm, a2,a3,a4 : integer;

FUNCTION testIfThenElseNested;
BEGIN   
   aa := 3;
   cc := 6;
   a1 := 1;
   b1 := 10;
   c1 := 100;
   xx := 0;
   d1 := (a1+b1)*((b1+c1)*a1);
   e1 := (a1+b1)*((b1+c1)*a1)*xx;
   dd := (aa + cc);
   dd := cc + aa;
   dd := ee + ff;
   z1 := dd*a1;
   y1 := z1/a1;
   dd := y1+y1;
   ee := 2*y1;

   aa := bb+cc;
   ff := aa;
   gg := ff;
   kk := bb+cc;
   ll := 1+bb;
   aa := 1;
   mm := aa+bb;
   a1 := 1 - 2 + 5 * 10;
   a2 := 4 - 2;
   b1 := a1

END
   
END   
.

