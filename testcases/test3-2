program testDemo;

class testDemo
BEGIN
 
   VAR aa, bb, cc, dd, kk, ee, ff, gg, hh, ii, jj : integer;

FUNCTION testDemo;
BEGIN
	aa := bb + cc;
	dd := ee - ff;
        kk := dd+ee;
	jj := aa * dd;
	hh := 10;
	ii := 5;

   
	if (cc + bb < ee - ff) then
	begin
		gg := hh - ii
	end
	else if ((bb + cc)  <  (dd * aa)) then
	begin
		hh := ii + jj;
		ee := aa;
		if bb + cc < dd + ee then
		begin
			if hh > ii then
				ii := ii + 1
			else
				ii := ii + 2;
			ee := ee - ff
		end
                else
                        aa := 5;
		hh := ii + jj
	end	
	else
		aa := gg +  dd;
	
	if true then
		if false then
			while true do
				while false do
					ii := ii + 1
		else
			aa := 2
	else
		dd := 3

END

END
.
