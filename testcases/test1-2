program testCases;

class testCases

BEGIN
	VAR
		a1,a2,a3,a4,a5,a6,a7,a8 : integer;

	FUNCTION testCases;
	BEGIN
		a1 := 1;          (* a1 = 1      *)
		a2 := 2;          (* a2 = 2      *)
		a3 := a1+a2;      (* a3 = 3      *)
		a4 := a3-3;       (* a4 = 0      *)
		a5 := a4*a2;      (* a5 = 0      *)
		a6 := a4+a6;      (* a6 = a6     *)
		a7 := a1 + a7;    (* a7 = 1 + a7 *)
		a8 := a8 + a3     (* a8 = a8 + 3 *)

	END
END
.

